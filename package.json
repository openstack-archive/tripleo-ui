{
  "name": "tripleo-ui",
  "version": "7.0.0",
  "description": "TripleO UI - TripleO-based OpenStack deployment UI",
  "main": "index.js",
  "dependencies": {
    "babel-core": "^6.0.0",
    "babel-polyfill": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "~2.2.5",
    "formatjs": "^0.1.1",
    "formsy-react": "~0.18.0",
    "immutable": "~3.8.1",
    "invariant": "~2.2.1",
    "js-yaml": "^3.7.0",
    "keymirror": "~0.1.1",
    "lodash": "^4.0.0",
    "node-uuid": "^1.4.7",
    "normalizr": "^2.0.0",
    "patternfly": "~3.3.5",
    "react": "~15.0.2",
    "react-bootstrap": "^0.30.8",
    "react-cookie": "^1.0.4",
    "react-dom": "~15.0.2",
    "react-immutable-proptypes": "~1.7.1",
    "react-intl": "2.1.2",
    "react-motion": "^0.4.4",
    "react-portal": "^2.2.1",
    "react-redux": "~4.4.5",
    "react-router": "~2.4.0",
    "redux": "~3.5.2",
    "redux-form": "^6.6.1",
    "redux-logger": "~2.6.1",
    "redux-thunk": "~2.1.0",
    "reqwest": "~2.0.1",
    "reselect": "^2.0.3",
    "shortid": "^2.2.4",
    "when": "~3.7.3",
    "xhr2": "~0.1.2"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-jest": "19.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-react-intl": "^2.2.0",
    "css-loader": "~0.23.1",
    "es6-promise": "~3.2.1",
    "eslint": "2.10.2",
    "eslint-plugin-react": "5.1.1",
    "file-loader": "~0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "19.0.2",
    "json-loader": "^0.5.4",
    "less": "~2.7.1",
    "less-loader": "~2.2.3",
    "react-addons-test-utils": "~15.0.2",
    "react-intl-po": "^1.1.0",
    "style-loader": "~0.13.1",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.0",
    "webpack-dev-server": "~1.14.1"
  },
  "scripts": {
    "build": "webpack --bail",
    "lint": "eslint --max-warnings 0 src",
    "start": "webpack-dev-server --progress",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "json2pot": "rip json2pot ./i18n/extracted-messages/**/*.json -o ./i18n/messages.pot",
    "po2json": "rip po2json -m ./i18n/extracted-messages/**/*.json",
    "docs": "cd docs && make html && cd -"
  },
  "jest": {
    "testURL": "http://localhost",
    "testMatch": [
      "**/__tests__/**/*.tests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openstack/tripleo-ui"
  },
  "keywords": [
    "tripleo",
    "tripleo-ui",
    "openstack"
  ],
  "author": "Openstack <openstack-dev@lists.openstack.org>",
  "license": "Apache-2.0"
}
